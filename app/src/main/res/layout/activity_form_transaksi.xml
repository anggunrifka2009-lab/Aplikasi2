tent"            android:orientation="horizontal"            android:paddingStart="35dp"            android:paddingTop="10dp">            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="Mie Bangladesh"                android:fontFamily="@font/poppins_regular"                android:textSize="18dp"/>            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="Rp 13.000"                android:fontFamily="@font/poppins_regular"                android:textSize="18dp"                android:layout_marginStart="25dp" />            <EditText                android:id="@+id/etMie"                android:layout_width="48dp"                android:layout_height="48dp"                android:hint="JML"                android:layout_marginStart="68dp"/>        </LinearLayout>        <TextView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:paddingStart="20dp"            android:paddingTop="5dp"            android:text="Minuman"            android:fontFamily="@font/poppins_bold"            android:textSize="20dp"            android:textStyle="bold" />        <LinearLayout            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:orientation="horizontal"            android:paddingStart="35dp"            android:paddingTop="10dp">            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="Es Teh"                android:fontFamily="@font/poppins_regular"                android:textSize="18dp" />            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"               